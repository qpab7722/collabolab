<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.collabolab">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        <activity android:name=".SetDateActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SetPeopleActivity" />
        <activity android:name=".SetTypeActivity" />
        <activity android:name=".LoginActivity"></activity>
        <activity
            android:name=".RoomReservationActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".ChooseTImeActivity"></activity>
    </application>

</manifest>