<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.collabolab">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
        android:largeHeap="true">
        <activity android:name=".SetDateActivity">
        </activity>
        <activity android:name=".SetPeopleActivity" />
        <activity android:name=".SetTypeActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RoomReservationActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".ChooseTImeActivity" />
        <activity
            android:name=".PutPurposeActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".ShowActivity" />
        <activity android:name=".MyInfoActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>